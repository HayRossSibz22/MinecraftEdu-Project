while true do
local success, blockbelow =  turtle.inspectDown()
 
if blockbelow.name == "minecraft:cobblestone" then
  turtle.forward()
elseif blockbelow.name == "minecaft:brick_block" then
 for n =1, 16 do
 
   turtle.suck()
   
  end
 turtle.turnLeft()
 turtle.turnLeft()
 turtle.forward()
 
 elseif blockbelow.name == "minecraft:redstone_block" then
 turtle.turnRight()
 turtle.turnRight()
 turtle.forward()
 
 else
turtle.turnRight()
turtle.forward()
end
end
